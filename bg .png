						if(discoverStrip.classList.contains("stick-home-discover-strip")) {
							discoverStrip.classList.remove("stick-home-discover-strip")
							discoverStripPlaceHolder.style.height = '0px';
							stickyHeaderElement.style.display = null
						}

						if(searchBar.classList.contains('stick-home-search-bar')) {
							searchBar.classList.remove('stick-home-search-bar')
						}
					}

					if(stickCondition) {
						if(window.scrollY > prevScroll) {
							if(prevScrollDirection !== 'down') {
								searchBar.style.visibility = 'hidden'
								prevScrollDirection = 'down'
							}
						} else {
							if(prevScrollDirection !== 'up') {
								searchBar.style.visibility = 'visible'
								prevScrollDirection = 'up'
							}
						}
					}
