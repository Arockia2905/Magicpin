				let searchBar = document.querySelector('.mobile-header .search-bar-holder')
				let stickyHeaderElement = document.querySelector('.mobile-header .sticky-header')
				let initialOffset = discoverStrip.getBoundingClientRect().top
				initialOffset = initialOffset > 0 ? initialOffset : 440
				let prevScroll = window.scrollY
				let prevScrollDirection = null
				let stickCondition = false
				window.addEventListener("scroll", () => {
					if(window.scrollY > initialOffset) {
						stickCondition = true;
						discoverStrip.classList.add("stick-home-discover-strip")
						discoverStripPlaceHolder.style.height = discoverStrip.offsetHeight + 'px';
						searchBar.classList.add('stick-home-search-bar')
